{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2020",
        "allowImportingTsExtensions": false,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "sourceMap": true,
        "strict": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}